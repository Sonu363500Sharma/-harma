#include<stdio.h>
void main()
{
    int i,n;
    float x,sum,t;
    printf("enter the number");
    scanf("%d",&n);
    printf("enter the value of x");
    scanf("%f",&x);
    x=x*3.14/180;
    t=x;
    sum=x;
    for(i=1;i<=n;i++)
    {
        t=t*(-1)*x*x/2*i*(2*i*1);
        sum=sum+t;
        printf("%d itration result is %t",i,sum);
    }
    printf("The value of sin=%t",n,sum);
    
}
